extends layout

block content
  h1 All Videos
  for vid in allVideos
    .embeded-video
      h2
        a(href='/tube/' + vid._id)= vid.name
      iframe(src=vid.url, height='390', width='480', title='YouTube Video Player')
      p Posted by: Fill in user name here!!!!!!#{postedBy}
  br
  a(href='/') Home
  br
  //- a(href='/tube/new-video/' + userId) Add Video
